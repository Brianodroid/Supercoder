#include<stdio.h>
#include<conio.h>
void swap(int a,int b);          
 
int main()
{
    int m = 22, n = 44;
    printf(" values before swap  m = %d \nand n = %d", m, n);
    return swap(m, n);                         
}
 
void swap(int a, int b)
{ 
    int tmp;
    tmp = a;
    a = b;
    b = tmp;
    printf(" \nvalues after swap m = %d\n and n = %d", a, b);
    getch() 
} 


